{
  "name": "codeanalysis",
  "displayName": "codeanalysis",
  "description": "Render information from various code analysis tools. Starting with PMD-CPD and Spotbugs.",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git@github.com:MikeNeilson/codeanalysis-gutters.git"
  },
  "license": "CC0 1.0 Universal",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other",
    "Linters",
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "codeanalysis.pmd.showDuplicates",
        "title": "Show Duplicate Code",
        "category": "Code Analysis-PMD"
      },
      {
        "command": "codeanalysis.pmd.hideDuplicates",
        "title": "Hide Duplicate Code Indicators",
        "category": "Code Analysis-PMD"
      },
      {
        "command": "codeanalysis.pmd.refreshDuplicates",
        "title": "Refresh Duplicate Tree",
        "category": "Code Analysis-PMD"
      },
      {
        "command": "codeanalysis.spotbugs.showBugs",
        "title": "Show Spotbugs data",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.hideBugs",
        "title": "Hide Spotbugs Data",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.refreshBugs",
        "title": "Refresh Spotbugs",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.Confidence",
        "title": "Select Bug Confidence",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.showLow",
        "title": "-  Low",
        "enablement": "view == spotbugs.Bugs",
        "icon": "blank",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.hideLow",
        "title": "✓ Low",
        "icon": "check",
        "enablement": "view == spotbugs.Bugs",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.showNormal",
        "title": "-  Normal",
        "enablement": "view == spotbugs.Bugs",
        "icon": "blank",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.hideNormal",
        "title": "✓  Normal",
        "icon": "check",
        "enablement": "view == spotbugs.Bugs",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.showHigh",
        "title": "-  High",
        "enablement": "view == spotbugs.Bugs",
        "icon": "blank",
        "category": "Code Analysis-Spotbugs"
      },
      {
        "command": "codeanalysis.spotbugs.confidence.hideHigh",
        "title": "✓  High",
        "icon": "check",
        "enablement": "view == spotbugs.Bugs",
        "category": "Code Analysis-Spotbugs"
      }
    ],
    "views": {
      "code-analysis": [
        {
          "id": "cpd.DuplicateCode",
          "name": "Duplicate Code"
        },
        {
          "id": "spotbugs.Bugs",
          "name": "Spotbugs"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-analysis",
          "title": "Code Analysis",
          "icon": "$(bug)"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "cpd.DuplicateCode",
        "contents": "No duplicate files found or not processed yet."
      },
      {
        "view": "spotbugs.Bugs",
        "contents": "No Spotbugs files found or not processed yet."
      }
    ],
    "menus": {
      "view/title": [
        {
          "when": "view == spotbugs.Bugs",
          "submenu": "spotbugs.filter"
        }
      ],
      "spotbugs.filter": [
        {
          "submenu": "spotbugs.filter.confidence"
        }
      ],
      "spotbugs.filter.confidence": [
        {
          "command": "codeanalysis.spotbugs.confidence.showLow",
          "when": "codeanalysis.spotbugs.confidence.low.show == false",
          "group": "confidence@1"
        },
        {
          "command": "codeanalysis.spotbugs.confidence.hideLow",
          "when": "codeanalysis.spotbugs.confidence.low.show == true",
          "group": "confidence@1"
        },
        {
          "command": "codeanalysis.spotbugs.confidence.showNormal",
          "when": "codeanalysis.spotbugs.confidence.normal.show == false",
          "group": "confidence@2"
        },
        {
          "command": "codeanalysis.spotbugs.confidence.hideNormal",
          "when": "codeanalysis.spotbugs.confidence.normal.show == true",
          "group": "confidence@2"
        },
        {
          "command": "codeanalysis.spotbugs.confidence.showHigh",
          "when": "codeanalysis.spotbugs.confidence.high.show == false",
          "group": "confidence@3"
        },
        {
          "command": "codeanalysis.spotbugs.confidence.hideHigh",
          "when": "codeanalysis.spotbugs.confidence.high.show == true",
          "group": "confidence@3"
        }
      ]
    },
    "submenus": [
      {
        "id": "spotbugs.filter",
        "label": "Filter"
      },
      {
        "id": "spotbugs.filter.confidence",
        "label": "Confidence"
      }
    ],
    "colors": [
      {
        "id": "duplicateStatus.critical",
        "description": "Background highlight color to make large sections of duplicated code obvious.",
        "defaults": {
          "dark": "#fa4d5620",
          "light": "#da1e2820"
        }
      },
      {
        "id": "duplicateStatus.major",
        "description": "Background highlight color to make medium sections of duplicated code obvious.",
        "defaults": {
          "dark": "#ff832b20",
          "light": "#ff832b20"
        }
      },
      {
        "id": "duplicateStatus.minor",
        "description": "Background highlight color to make small sections of duplicated code obvious.",
        "defaults": {
          "dark": "#f1c21b20",
          "light": "#f1c21b20"
        }
      },
      {
        "id": "spotbugs.highConfidence",
        "description": "Background highlight color to make large sections of duplicated code obvious.",
        "defaults": {
          "dark": "#fa4d5620",
          "light": "#da1e2820"
        }
      },
      {
        "id": "spotbugs.normalConfidence",
        "description": "Background highlight color to make medium sections of duplicated code obvious.",
        "defaults": {
          "dark": "#ff832b20",
          "light": "#ff832b20"
        }
      },
      {
        "id": "spotbugs.lowConfidence",
        "description": "Background highlight color to make small sections of duplicated code obvious.",
        "defaults": {
          "dark": "#f1c21b20",
          "light": "#f1c21b20"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "8.1.0",
    "@types/mocha": "10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "1.75.0",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "@vscode/test-electron": "2.2.3",
    "eslint": "8.34.0",
    "glob": "8.1.0",
    "mocha": "10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@types/xml2js": "0.4.11",
    "@vscode/codicons": "^0.0.32",
    "xml2js": "0.4.23"
  },
  "vsce": {
    "dependencies": true,
    "yarn": false
  }
}
